@charset "utf-8";

#clock { border: 30px dashed rgb(153, 153, 153); width: 700px; height: 700p=
x; position: relative; left: 0px; top: 0px; }

#hour, #min, #sec { border: 16px solid rgb(85, 85, 85); width: 50px; positi=
on: absolute; border-radius: 50px; }

#hour > div, #min > div, #sec > div { font-variant: small-caps; font-weight=
: bold; color: white; text-align: center; margin-top: 20px; }

#hour { background-color: tomato; height: 150px; left: 309px; top: 168px; t=
ransition-property: all; transition-duration: 21600s; transition-timing-fun=
ction: linear; transform-origin: center bottom; }

#min { background-color: cornflowerblue; height: 220px; left: 309px; top: 9=
8px; transition-property: all; transition-duration: 360s; transition-timing=
-function: linear; transform-origin: center bottom; }

#sec { background-color: gold; height: 290px; left: 309px; top: 28px; trans=
ition-property: all; transition-duration: 60s; transition-timing-function: =
linear; transform-origin: center bottom; }

#clock:hover #sec { transform: rotate(36000deg); }

#clock:hover #min { transform: rotate(36000deg); }

#clock:hover #hour { transform: rotate(36000deg); }
